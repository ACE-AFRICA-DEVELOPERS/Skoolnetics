include includes/header.pug 
block content
    link(rel="stylesheet" href="/stylesheets/dashboard.css")
    body
        .loader-bg
            .loader-track
                .loader-fill
        include includes/sidenav.pug 
        include includes/usernav.pug
        .pcoded-main-container
            .pcoded-wrapper
                .pcoded-content
                    .pcoded-inner-content
                        .main-body
                            .page-wrapper
                                .card
                                    if results.length > 0
                                        .card-header
                                            h5 CBT Results
                                            span.d-block.m-t-5 Select the result you want to see.
                                        .card-body
                                            .row
                                                each result in results
                                                    .col-md-4
                                                        - let courseUrl = '/student/cbt-result/' + result._id
                                                        a(href=courseUrl class="text-dark")
                                                            .field-box
                                                                p #[b #{examName[result.exam]}]
                                                               
                                            
                                    else
                                        .card-header
                                            h5 CBT Results
                                            span.d-block.m-t-5 None of your results has been released.          

        include includes/footer.pug